{
  "name": "vue-airbnb-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier -w src/**/*.{ts,vue}",
    "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore",
